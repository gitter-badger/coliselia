{
  "name": "dbapi",
  "version": "1.0.0",
  "description": "Database Interactions API for Coliselia",
  "main": "bin/server.js",
  "scripts": {
    "start": "node bin/server.js",
    "test": "echo 'TEST GO HERE'",
    "predocs": "npm run compile || true",
    "docs": "npm run docs:src && npm run docs:api",
    "docs:src": "jsdoc bin/ --destination docs/src/",
    "docs:api": "apidoc --input bin/ --output docs/api/",
    "precompile": "mkdir -p bin",
    "compile": "tsc",
    "watch:compile": "watch 'npm run compile' src/",
    "setup": "npm-run-all setup:*",
    "setup:compile": "npm run compile || true",
    "clean": "rm -rf bin/ node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/coliselia.git"
  },
  "author": "siggame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/coliselia/issues"
  },
  "homepage": "https://github.com/siggame/coliselia#readme",
  "devDependencies": {
    "apidoc": "^0.16.1",
    "jsdoc": "^3.4.3",
    "npm-run-all": "^3.1.2",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "watch": "^1.0.1"
  },
  "dependencies": {
    "@types/bluebird": "^3.0.36",
    "@types/body-parser": "0.0.33",
    "@types/config": "0.0.31",
    "@types/express": "^4.0.34",
    "@types/jsonschema": "0.0.5",
    "@types/knex": "0.0.36",
    "@types/lodash": "^4.14.42",
    "@types/winston": "0.0.32",
    "body-parser": "^1.15.2",
    "config": "^1.24.0",
    "express": "^4.14.0",
    "js-yaml": "^3.7.0",
    "jsonschema": "^1.1.0",
    "knex": "^0.12.6",
    "lodash": "^4.17.2",
    "pg": "^6.1.0",
    "winston": "^2.3.0"
  },
  "private": true
}
